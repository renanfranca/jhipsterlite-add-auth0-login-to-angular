{
  "name": "baby",
  "version": "0.0.0",
  "private": true,
  "description": "Mamazinha Baby Care",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.12.1"
  },
  "jestSonar": {
    "reportPath": "target/test-results",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path=target/classes/static",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --coverage",
    "lint": "ng lint",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\""
  },
  "devDependencies": {
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "8.27.0",
    "@angular-builders/jest": "14.0.2",
    "@angular-devkit/build-angular": "14.2.9",
    "@angular/cli": "14.2.9",
    "@angular/compiler-cli": "14.2.10",
    "@types/node": "18.11.9",
    "@types/jest": "29.2.2",
    "jest": "28.1.3",
    "jest-environment-jsdom": "29.3.1",
    "ts-jest": "28.0.8",
    "jest-preset-angular": "12.2.2",
    "jest-sonar-reporter": "2.0.0",
    "typescript": "4.8.4",
    "@prettier/plugin-xml": "2.2.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-java": "1.6.2",
    "prettier-plugin-packagejson": "2.3.0"
  },
  "dependencies": {
    "@angular/animations": "14.2.10",
    "@angular/cdk": "14.2.7",
    "@angular/common": "14.2.10",
    "@angular/compiler": "14.2.10",
    "@angular/core": "14.2.10",
    "@angular/material": "14.2.7",
    "@angular/forms": "14.2.10",
    "@angular/platform-browser": "14.2.10",
    "@angular/platform-browser-dynamic": "14.2.10",
    "@angular/router": "14.2.10",
    "rxjs": "7.5.7",
    "tslib": "2.4.1",
    "zone.js": "0.12.0"
  }
}
